#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00107AE8 .scope module, "RegisterFile" "RegisterFile" 2 1;
 .timescale 0 0;
v009DB2D8_0 .var "IN", 7 0;
v009DB070_0 .var "INaddr", 2 0;
v009DB280_0 .net "OUT1", 7 0, v001016C0_0; 1 drivers
v009DAEB8_0 .var "OUT1addr", 2 0;
v009DB178_0 .net "OUT2", 7 0, v00101718_0; 1 drivers
v009DB330_0 .var "OUT2addr", 2 0;
v009DAF68_0 .var "RESET", 0 0;
v009DB388_0 .var "clk", 0 0;
S_00107FB0 .scope module, "REGISTER" "reg_file" 2 8, 2 35, S_00107AE8;
 .timescale 0 0;
v0010EC18_0 .net "IN", 7 0, v009DB2D8_0; 1 drivers
v009A3210_0 .net "INaddr", 2 0, v009DB070_0; 1 drivers
v009A3268_0 .alias "OUT1", 7 0, v009DB280_0;
v0010EC70_0 .net "OUT1addr", 2 0, v009DAEB8_0; 1 drivers
v0010ECC8_0 .alias "OUT2", 7 0, v009DB178_0;
v009B2E70_0 .net "OUT2addr", 2 0, v009DB330_0; 1 drivers
v009B2EC8_0 .net "RESET", 0 0, v009DAF68_0; 1 drivers
v009B2F20_0 .net "clk", 0 0, v009DB388_0; 1 drivers
v00101668 .array "registers", 0 7, 7 0;
v001016C0_0 .var "temp", 7 0;
v00101718_0 .var "temp1", 7 0;
E_00108D80 .event posedge, v009B2F20_0;
E_00108DE0 .event negedge, v009B2F20_0;
    .scope S_00107FB0;
T_0 ;
   %ix/load 1, 0, 0;
   %ix/load 3, 0, 0;
   %set/av v00101668, 0, 8;
    %movi 8, 1, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 1, 0;
   %set/av v00101668, 8, 8;
    %movi 8, 2, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 2, 0;
   %set/av v00101668, 8, 8;
    %movi 8, 3, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 3, 0;
   %set/av v00101668, 8, 8;
    %movi 8, 4, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 4, 0;
   %set/av v00101668, 8, 8;
    %movi 8, 5, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 5, 0;
   %set/av v00101668, 8, 8;
    %movi 8, 6, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 6, 0;
   %set/av v00101668, 8, 8;
    %movi 8, 7, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 7, 0;
   %set/av v00101668, 8, 8;
    %end;
    .thread T_0;
    .scope S_00107FB0;
T_1 ;
    %wait E_00108DE0;
    %load/v 8, v0010EC18_0, 8;
    %ix/getv 3, v009A3210_0;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v00101668, 8, 8;
t_0 ;
    %vpi_call 2 62 "$display", "negetive edge";
    %vpi_call 2 63 "$display", "Value of INaddr : %b ", &A<v00101668, v009A3210_0 >;
    %jmp T_1;
    .thread T_1;
    .scope S_00107FB0;
T_2 ;
    %wait E_00108D80;
    %ix/getv 3, v0010EC70_0;
    %load/av 8, v00101668, 8;
    %set/v v001016C0_0, 8, 8;
    %ix/getv 3, v009B2E70_0;
    %load/av 8, v00101668, 8;
    %set/v v00101718_0, 8, 8;
    %vpi_call 2 69 "$display", "positive edge";
    %vpi_call 2 70 "$display", "Value of INaddr : %b ", &A<v00101668, v009A3210_0 >;
    %jmp T_2;
    .thread T_2;
    .scope S_00107AE8;
T_3 ;
    %delay 5, 0;
    %load/v 8, v009DB388_0, 1;
    %inv 8, 1;
    %set/v v009DB388_0, 8, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_00107AE8;
T_4 ;
    %delay 20, 0;
    %vpi_call 2 14 "$finish";
    %end;
    .thread T_4;
    .scope S_00107AE8;
T_5 ;
    %set/v v009DB388_0, 1, 1;
    %delay 5, 0;
    %movi 8, 1, 3;
    %set/v v009DAEB8_0, 8, 3;
    %set/v v009DB330_0, 0, 3;
    %movi 8, 5, 3;
    %set/v v009DB070_0, 8, 3;
    %set/v v009DAF68_0, 1, 1;
    %set/v v009DB2D8_0, 1, 8;
    %vpi_call 2 20 "$monitor", "Out 1 = %b Out 2 = %b OUT1addr = %b OUT2addr = %b IN = %b clock = %b \012", v009DB280_0, v009DB178_0, v009DAEB8_0, v009DB330_0, v009DB2D8_0, v009DB388_0;
    %movi 8, 1, 3;
    %set/v v009DAEB8_0, 8, 3;
    %set/v v009DB330_0, 0, 3;
    %movi 8, 5, 3;
    %set/v v009DB070_0, 8, 3;
    %set/v v009DAF68_0, 1, 1;
    %movi 8, 247, 8;
    %set/v v009DB2D8_0, 8, 8;
    %delay 10, 0;
    %vpi_call 2 23 "$monitor", "Out 1 = %b Out 2 = %b OUT1addr = %b OUT2addr = %b IN = %b clock = %b \012", v009DB280_0, v009DB178_0, v009DAEB8_0, v009DB330_0, v009DB2D8_0, v009DB388_0;
    %movi 8, 1, 3;
    %set/v v009DAEB8_0, 8, 3;
    %set/v v009DB330_0, 0, 3;
    %movi 8, 5, 3;
    %set/v v009DB070_0, 8, 3;
    %set/v v009DAF68_0, 1, 1;
    %movi 8, 7, 8;
    %set/v v009DB2D8_0, 8, 8;
    %delay 15, 0;
    %vpi_call 2 26 "$monitor", "Out 1 = %b Out 2 = %b OUT1addr = %b OUT2addr = %b IN = %b clock = %b \012", v009DB280_0, v009DB178_0, v009DAEB8_0, v009DB330_0, v009DB2D8_0, v009DB388_0;
    %movi 8, 1, 3;
    %set/v v009DAEB8_0, 8, 3;
    %set/v v009DB330_0, 0, 3;
    %movi 8, 1, 3;
    %set/v v009DB070_0, 8, 3;
    %set/v v009DAF68_0, 1, 1;
    %movi 8, 149, 8;
    %set/v v009DB2D8_0, 8, 8;
    %delay 20, 0;
    %vpi_call 2 29 "$monitor", "Out 1 = %b Out 2 = %b OUT1addr = %b OUT2addr = %b IN = %b clock = %b \012", v009DB280_0, v009DB178_0, v009DAEB8_0, v009DB330_0, v009DB2D8_0, v009DB388_0;
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "RegisterFile.v";
