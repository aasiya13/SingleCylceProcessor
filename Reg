#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00B87C80 .scope module, "RegisterFile" "RegisterFile" 2 1;
 .timescale 0 0;
v009E10E0_0 .var "IN", 7 0;
v009E0F28_0 .var "INaddr", 2 0;
v009E0CC0_0 .net "OUT1", 7 0, v009E0ED0_0; 1 drivers
v009E1088_0 .var "OUT1addr", 2 0;
v009E1138_0 .net "OUT2", 7 0, v009E13A0_0; 1 drivers
v009E1190_0 .var "OUT2addr", 2 0;
v009E13F8_0 .var "RESET", 0 0;
v009E11E8_0 .var "clk", 0 0;
S_00B88038 .scope module, "REGISTER" "reg_file" 2 8, 2 33, S_00B87C80;
 .timescale 0 0;
v009D2940_0 .net "IN", 7 0, v009E10E0_0; 1 drivers
v009D2998_0 .net "INaddr", 2 0, v009E0F28_0; 1 drivers
v009D3AD8_0 .alias "OUT1", 7 0, v009E0CC0_0;
v009D3B30_0 .net "OUT1addr", 2 0, v009E1088_0; 1 drivers
v00B8B820_0 .alias "OUT2", 7 0, v009E1138_0;
v00B8B878_0 .net "OUT2addr", 2 0, v009E1190_0; 1 drivers
v009E0F80_0 .net "RESET", 0 0, v009E13F8_0; 1 drivers
v009E1030_0 .net "clk", 0 0, v009E11E8_0; 1 drivers
v009E0FD8 .array "registers", 0 7, 7 0;
v009E0ED0_0 .var "temp", 7 0;
v009E13A0_0 .var "temp1", 7 0;
E_00B8E838 .event posedge, v009E1030_0;
E_00B8E9B8 .event negedge, v009E1030_0;
    .scope S_00B88038;
T_0 ;
   %ix/load 1, 0, 0;
   %ix/load 3, 0, 0;
   %set/av v009E0FD8, 0, 8;
    %movi 8, 1, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 1, 0;
   %set/av v009E0FD8, 8, 8;
    %movi 8, 2, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 2, 0;
   %set/av v009E0FD8, 8, 8;
    %movi 8, 3, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 3, 0;
   %set/av v009E0FD8, 8, 8;
    %movi 8, 4, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 4, 0;
   %set/av v009E0FD8, 8, 8;
    %movi 8, 5, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 5, 0;
   %set/av v009E0FD8, 8, 8;
    %movi 8, 6, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 6, 0;
   %set/av v009E0FD8, 8, 8;
    %movi 8, 7, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 7, 0;
   %set/av v009E0FD8, 8, 8;
    %end;
    .thread T_0;
    .scope S_00B88038;
T_1 ;
    %wait E_00B8E9B8;
    %load/v 8, v009D2940_0, 8;
    %ix/getv 3, v009D2998_0;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v009E0FD8, 8, 8;
t_0 ;
    %vpi_call 2 60 "$display", "negetive edge";
    %vpi_call 2 61 "$display", "Value of INaddr : %b ", &A<v009E0FD8, v009D2998_0 >;
    %jmp T_1;
    .thread T_1;
    .scope S_00B88038;
T_2 ;
    %wait E_00B8E838;
    %ix/getv 3, v009D3B30_0;
    %load/av 8, v009E0FD8, 8;
    %set/v v009E0ED0_0, 8, 8;
    %ix/getv 3, v00B8B878_0;
    %load/av 8, v009E0FD8, 8;
    %set/v v009E13A0_0, 8, 8;
    %vpi_call 2 67 "$display", "positive edge";
    %vpi_call 2 68 "$display", "Value of INaddr : %b ", &A<v009E0FD8, v009D2998_0 >;
    %jmp T_2;
    .thread T_2;
    .scope S_00B87C80;
T_3 ;
    %movi 8, 1, 3;
    %set/v v009E1088_0, 8, 3;
    %set/v v009E1190_0, 0, 3;
    %movi 8, 5, 3;
    %set/v v009E0F28_0, 8, 3;
    %set/v v009E13F8_0, 1, 1;
    %set/v v009E10E0_0, 1, 8;
    %set/v v009E11E8_0, 1, 1;
    %delay 5, 0;
    %vpi_call 2 18 "$display", "Out 1 = %b Out 2 = %b OUT1addr = %b OUT2addr = %b IN = %b clock = %b \012", v009E0CC0_0, v009E1138_0, v009E1088_0, v009E1190_0, v009E10E0_0, v009E11E8_0;
    %movi 8, 1, 3;
    %set/v v009E1088_0, 8, 3;
    %set/v v009E1190_0, 0, 3;
    %movi 8, 5, 3;
    %set/v v009E0F28_0, 8, 3;
    %set/v v009E13F8_0, 1, 1;
    %movi 8, 247, 8;
    %set/v v009E10E0_0, 8, 8;
    %set/v v009E11E8_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 21 "$display", "Out 1 = %b Out 2 = %b OUT1addr = %b OUT2addr = %b IN = %b clock = %b \012", v009E0CC0_0, v009E1138_0, v009E1088_0, v009E1190_0, v009E10E0_0, v009E11E8_0;
    %movi 8, 1, 3;
    %set/v v009E1088_0, 8, 3;
    %set/v v009E1190_0, 0, 3;
    %movi 8, 5, 3;
    %set/v v009E0F28_0, 8, 3;
    %set/v v009E13F8_0, 1, 1;
    %movi 8, 7, 8;
    %set/v v009E10E0_0, 8, 8;
    %set/v v009E11E8_0, 1, 1;
    %delay 15, 0;
    %vpi_call 2 24 "$display", "Out 1 = %b Out 2 = %b OUT1addr = %b OUT2addr = %b IN = %b clock = %b \012", v009E0CC0_0, v009E1138_0, v009E1088_0, v009E1190_0, v009E10E0_0, v009E11E8_0;
    %movi 8, 1, 3;
    %set/v v009E1088_0, 8, 3;
    %set/v v009E1190_0, 0, 3;
    %movi 8, 1, 3;
    %set/v v009E0F28_0, 8, 3;
    %set/v v009E13F8_0, 1, 1;
    %movi 8, 149, 8;
    %set/v v009E10E0_0, 8, 8;
    %set/v v009E11E8_0, 0, 1;
    %delay 20, 0;
    %vpi_call 2 27 "$display", "Out 1 = %b Out 2 = %b OUT1addr = %b OUT2addr = %b IN = %b clock = %b \012", v009E0CC0_0, v009E1138_0, v009E1088_0, v009E1190_0, v009E10E0_0, v009E11E8_0;
    %end;
    .thread T_3;
